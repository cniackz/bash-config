openssl req -new -config operator.cnf -key private.key -out operator.csr

cat operator.csr | base64 | tr -d "\n"


root@ubuntu:/# cat operator.csr | base64 | tr -d "\n"



root@ubuntu:/# 
root@ubuntu:/# 


kubectl certificate approve operator-csr

k get csr operator-csr -o jsonpath='{.status.certificate}'| base64 -d > public.crt



